{
  "name": "Play2Twitter",
  "manifest_version": 2,
  "version": "0.1",
  "page_action": {
    "default_icon": "icons/navbar.png",
    "default_title": "Share this song on Twitter"
  },
  "background": {
    "scripts": ["js/background.js"]
  },
  "permissions" : [
    "tabs",
    "*://play.google.com/music/listen*"
  ],
  "icons": {
    "16": "icons/icon-16.png",
    "48": "icons/icon-48.png",
    "128": "icons/icon-128.png"
  }
}